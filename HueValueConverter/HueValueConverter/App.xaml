<Application x:Class="HueValueConverter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HueValueConverter"
             xmlns:cnv="clr-namespace:HueValueConverter.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <cnv:DecimalHexadecimalConverter x:Key="decimalHexadecimalConverter"/>
        <cnv:RoundingConverter x:Key="roundingConverter"/>
        <cnv:ColorConverter x:Key="colorConverter"/>
    </Application.Resources>
</Application>
